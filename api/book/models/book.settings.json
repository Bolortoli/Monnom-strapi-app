{
  "kind": "collectionType",
  "collectionName": "Book",
  "info": {
    "name": "Book",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": false
    },
    "read_count": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "has_sale": {
      "type": "boolean",
      "default": false
    },
    "sale_quantity": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "has_audio": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "has_pdf": {
      "type": "boolean",
      "default": false,
      "private": false,
      "required": false
    },
    "pdf_book_path": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "files"
      ],
      "plugin": "upload",
      "required": false
    },
    "book_categories": {
      "collection": "book-category"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "picture": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "book_comments": {
      "collection": "book-comment",
      "via": "book"
    },
    "is_featured": {
      "type": "boolean",
      "default": false
    },
    "youtube_intro": {
      "type": "string"
    },
    "picture_comment": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "introduction": {
      "type": "text"
    },
    "book_authors": {
      "collection": "book-author"
    },
    "test": {
      "model": "test"
    },
    "online_book_price": {
      "type": "integer",
      "required": false
    },
    "book_price": {
      "type": "integer",
      "required": false
    }
  }
}
